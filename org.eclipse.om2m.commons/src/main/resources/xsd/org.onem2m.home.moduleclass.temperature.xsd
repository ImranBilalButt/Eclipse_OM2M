<xs:schema targetNamespace="http://www.onem2m.org/xml/protocols"
	xmlns:m2m="http://www.onem2m.org/xml/protocols" elementFormDefault="unqualified"
	xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:include schemaLocation="CDT-FlexContainer.xsd" />

	<xs:element name="fcnt">
		<xs:complexType>
			<xs:all>
				<!-- expirationTime = et -->
				<xs:element name="et" minOccurs="0" maxOccurs="1"
					type="m2m:timestamp" />

				<!-- access control policy IDs = acpi -->
				<xs:element name="acpi" minOccurs="0" maxOccurs="1"
					type="m2m:acpType" />

				<!-- labels = lbl -->
				<xs:element name="lbl" minOccurs="0" maxOccurs="1"
					type="m2m:labels" />

				<!-- announceTo = at -->
				<xs:element name="at" minOccurs="0" maxOccurs="1" type="xs:anyURI" />

				<!-- announcedAttribute = aa -->
				<xs:element name="aa" minOccurs="0" maxOccurs="1" type="xs:NCName" />

				<!-- dynamic Authorization Consultation IDs -->
				<!-- TODO -->

				<!-- creator = cr -->
				<xs:element name="cr" minOccurs="0" maxOccurs="1" type="m2m:ID" />

				<!-- ontologyRef = or -->
				<xs:element name="or" minOccurs="0" maxOccurs="1" type="xs:anyURI" />

				<!-- container Definition = cntDef -->
				<xs:element name="cntDef" minOccurs="0" maxOccurs="1" />

				<xs:element name="currentTemperature" minOccurs="0"
					maxOccurs="1">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:float">
								<xs:attribute name="type" type="xs:anySimpleType"
									use="required" fixed="xs:float"></xs:attribute>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>

				<xs:element name="targetTemperature" minOccurs="0"
					maxOccurs="1">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:float">
								<xs:attribute name="type" type="xs:anySimpleType"
									use="required" fixed="xs:float"></xs:attribute>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>

				<xs:element name="units" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:string">
								<xs:attribute name="type" type="xs:anySimpleType"
									use="required" fixed="xs:string"></xs:attribute>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>

				<xs:element name="minValue" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:float">
								<xs:attribute name="type" type="xs:anySimpleType"
									use="required" fixed="xs:float"></xs:attribute>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>

				<xs:element name="maxValue" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:float">
								<xs:attribute name="type" type="xs:anySimpleType"
									use="required" fixed="xs:float"></xs:attribute>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>

				<xs:element name="stepValue" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:float">
								<xs:attribute name="type" type="xs:anySimpleType"
									use="required" fixed="xs:float"></xs:attribute>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>

			</xs:all>
			<xs:attribute name="rn" type="xs:token" use="optional" /> <!-- rn = resourceName -->
		</xs:complexType>
	</xs:element>


</xs:schema>